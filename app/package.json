{
  "name": "react-static-example-basic",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "deref": "json-dereference -s ../api-docs/api.yaml -o public/api.json",
    "start": "npm run deref && react-static start",
    "stage": "npm run deref && react-static build --staging",
    "build": "npm run deref && react-static build",
    "bundle": "npm run deref && react-static bundle",
    "export": "npm run deref && react-static export",
    "serve": "serve dist -p 3000"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.16.2",
    "json-dereference-cli": "^0.1.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^4.0.0-beta.21",
    "react-jss": "^8.6.1",
    "react-router": "^4.2.0",
    "react-static": "^5.9.12"
  },
  "devDependencies": {
    "eslint-config-react-tools": "1.x.x",
    "serve": "^6.1.0"
  }
}
