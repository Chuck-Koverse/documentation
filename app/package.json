{
  "name": "react-static-example-basic",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "swagger": "swagger-cli bundle ../open-api/api.yaml -t json -o public/api.json && swagger-cli validate public/api.json",
    "pdf": "node ./scripts/pdf",
    "start": "npm run swagger && react-static start",
    "stage": "npm run swagger && react-static build --staging",
    "build": "npm run swagger && npm run pdf && react-static build",
    "bundle": "npm run swagger && react-static bundle",
    "export": "npm run swagger && react-static export",
    "serve": "serve dist -p 3000"
  },
  "dependencies": {
    "@koverse/components": "0.0.4",
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "arrify": "^1.0.1",
    "axios": "^0.16.2",
    "classnames": "^2.2.6",
    "jdown": "^0.6.5",
    "markdown-pdf": "^9.0.0",
    "marked": "^0.5.1",
    "prismjs": "^1.15.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^4.0.0-beta.21",
    "react-jss": "^8.6.1",
    "react-router": "^4.2.0",
    "react-static": "^5.9.12",
    "recompose": "^0.30.0",
    "slugify": "^1.3.1",
    "swagger-cli": "^2.2.0"
  },
  "devDependencies": {
    "eslint-config-react-tools": "1.x.x",
    "serve": "^6.1.0"
  }
}
